{
  "name": "driving-school-mvp",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "node prisma/seed/index.js"
  },
  "type": "commonjs",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "// Database Scripts": "===================",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed:fresh": "node prisma/seed/index.js --fresh",
    "db:seed:minimal": "node prisma/seed/index.js --minimal",
    "// Reset & Migration": "===================",
    "db:reset:fresh": "rm -rf prisma/migrations && prisma migrate reset --force --skip-seed && prisma db push && npm run db:seed:fresh",
    "db:reset:windows": "scripts\\reset-database.bat",
    "db:reset:unix": "sh scripts/reset-database.sh",
    "// Quick Setup": "===================",
    "setup": "npm install && npm run db:reset:fresh",
    "setup:docker": "docker-compose up -d && sleep 10 && npm run db:reset:fresh",
    "// Development Helpers": "===================",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "clean:windows": "rmdir /s /q .next node_modules && del package-lock.json && npm install",
    "update:deps": "npm update && npm audit fix",
    "// Testing": "===================",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:seed": "tsx prisma/seed.ts",
    "type-check": "tsc --noEmit",
    "prisma:validate": "npm run prisma:merge && npx prisma validate",
    "build": "npm run prisma:generate && next build",
    "db:setup": "npm run prisma:merge && npx prisma migrate dev --name init && npx prisma db seed",
    "db:reset": "npm run prisma:reset -- --force && npm run db:seed",
    "db:seed": "npx prisma db seed",
    "prisma:reset": "npm run prisma:merge && npx prisma migrate reset",
    "prisma:merge": "node scripts/merge-prisma-schemas.js",
    "prisma:generate": "npm run prisma:merge && npx prisma generate",
    "prisma:migrate": "npm run prisma:merge && npx prisma migrate dev",
    "prisma:push": "npm run prisma:merge && npx prisma db push",
    "prisma:studio": "npm run prisma:merge && npx prisma studio",
    "prisma:deploy": "npm run prisma:merge && npx prisma migrate deploy"
  },
